(* Да се напише асемблерска програма во која на променливите a,
b, c и d ќе им се доделат вредностите 8, 17, 39 и 106, а потоа
нивниот збир ќе се смести во променливата a. *)

addi $s0,$zero,8
addi $s1,$zero,17
addi $s2,$zero,39
addi $s3,$zero,106

add $t0,$s0,$s1
add $t1 $s2,$s3

add $s0,$t0,$t1



