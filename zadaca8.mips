(* Да се напише асемблерска програма која ќе го пресмета збирот
на сите позитивни парни двоцифрени броеви и ќе го смести
резултатот во $s0.
 *)
# $t0 = граница за циклус
# $t1 = бројач
# $s0 = збир
addi $t0, $zero, 98 # 98 е последниот позитивен парен двоцифрен број
addi $t1, $zero, 10 # 10 е првиот позитивен парен двоцифрен број
add $s0, $zero, $zero # не е неопходно, но пожелно за читливост
loop: bgt $t1, $t0, endLoop
add $s0, $s0, $t1
addi $t1, $t1, 2
j loop
endLoop:
